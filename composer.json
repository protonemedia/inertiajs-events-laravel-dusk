{
    "name": "protonemedia/inertiajs-events-laravel-dusk",
    "description": "Inertia.js Events for Laravel Dusk",
    "keywords": [
        "protonemedia",
        "laravel",
        "laravel dusk",
        "testing",
        "inertia",
        "inertiajs",
        "inertia.js",
        "inertiajs-events-laravel-dusk"
    ],
    "homepage": "https://github.com/protonemedia/inertiajs-events-laravel-dusk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^8.67||^9.0",
        "laravel/dusk": "^6.15|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^6.23||^7.0",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\InertiaJsEventsLaravelDusk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\InertiaJsEventsLaravelDusk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\InertiaJsEventsLaravelDusk\\ServiceProvider"
            ]
        }
    }
}